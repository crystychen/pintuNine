/* tab导航栏目 */
.toolbar-center{
  display: flex;
  justify-content: center;
  width:100%;
  position: fixed;
  /* bottom: 550rpx; */
  top: 670rpx;
  height:60rpx;
  z-index: 10001;
  
}
.toolbar-center view{
  display: inline-block;
  width:150rpx;
  height:60rpx;
  font-size: 30rpx;
  text-align: center;
  margin-top: 0rpx;
  position: relative;
  border-radius: 10rpx;
}
.toolbar-center view:active{
  display: inline-block;
  width:150rpx;
  height:60rpx;
  font-size: 33rpx;
  font-weight: bold;
  text-align: center;
  margin-top: 0rpx;
  position: relative;
  background-color: rgba(255,255,255,0.5);
  border-radius: 10rpx;
}
.toolbar-center view text{
  display: block;
  margin: 0 auto;
}
.toolbar-center view .text{
  position: absolute;
  bottom: 0;
  left:50%;
  transform: translateX(-50%);
  white-space: nowrap;
}