/* 下方按钮工具 */
.toolbar-bottom {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width:100%;
  position: fixed;
  bottom: 0;
  /* height:160rpx; */
}
.toolbar-bottom .row {
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around;
  margin: 10rpx 0;
}
.btn {
  border-radius: 50rpx;
  height: 80rpx;
  line-height: 80rpx;
  width: 300rpx;
  position: relative;
  text-align: center;
}
.btn.border {
  color: #FE9B0A;
  border: 1rpx solid #FE9B0A;
}
.btn.active {
  background: #FE9B0A;
  color: #fff;
}
.icon-btn {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  margin-bottom: 20rpx;
  margin-top: 10rpx;
}
.icon-btn > .text {
  color: #6D9FCB;
  font-size: 28rpx;
}
